    },
    "nodemon server.js": {
      "name": "nodemon server.js",
      "command": "nodemon server.js",
      "runAtStart": true
    },
    "node server.js": {
      "name": "node server.js",
      "command": "node server.js",
      "runAtStart": true
    }
  }
}
